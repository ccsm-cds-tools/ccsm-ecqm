        "url": "name",
        "valueString": "Initial Population"
      }, {
        "url": "statement",
        "valueString": "define \"Initial Population\":\n  AgeInYearsAt(date from start of \"Measurement Period\") in Interval[23, 65)\n    and TLSL.FemaleorTransgenderMale\n    and exists AdultOutpatient.\"Qualifying Encounters\""
      }, {
        "url": "displaySequence",
        "valueInteger": 10
      } ]
